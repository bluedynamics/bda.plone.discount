<registry>

  <records prefix="plone.bundles/bda-plone-discount"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="compile">False</value>
    <value key="jscompilation">++resource++bda.plone.discount.js</value>
    <value key="csscompilation">++resource++bda.plone.discount.css</value>
    <value key="depends">bdajax</value>
  </records>

  <!-- yafowil.plone.jqueryui -->
  <record name="yafowil.plone.jqueryui">
    <field type="plone.registry.field.Bool">
      <title>jqueryui for Plone</title>
    </field>
    <value>True</value>
  </record>

  <!-- yafowil.widget.autocomplete -->
  <record name="yafowil.widget.autocomplete.dependencies">
    <field type="plone.registry.field.Bool">
      <title>Autocomplete widget dependency resources</title>
    </field>
    <value>False</value>
  </record>

  <record name="yafowil.widget.autocomplete.common">
    <field type="plone.registry.field.Bool">
      <title>Autocomplete widget common resources</title>
    </field>
    <value>True</value>
  </record>

  <!-- yafowil.widget.array -->
  <record name="yafowil.widget.array.common">
    <field type="plone.registry.field.Bool">
      <title>Array widget common resources</title>
    </field>
    <value>True</value>
  </record>


</registry>